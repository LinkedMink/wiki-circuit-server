    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "~0.22.13",
    "@types/cors": "~2.8.6",
    "@types/express": "~4.17.1",
    "@types/jest": "~24.0.22",
    "@types/morgan": "~1.7.37",
    "@types/node-fetch": "~2.5.2",
    "cross-env": "~6.0.3",
    "jest": "~24.9.0",
    "ts-jest": "~24.1.0",
    "ts-node-dev": "~1.0.0-pre.43",
    "typescript": "~3.6.4"
