{
  "name": "wiki-circuit-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build && npm run startApp",
    "build": "tsc",
    "startApp": "node -r dotenv/config ./build/App.js",
    "watch": "ts-node-dev --respawn --transpileOnly ./app/App.ts"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cheerio": "~1.0.0-rc.3",
    "cors": "~2.8.5",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "morgan": "~1.9.1",
    "node-fetch": "~2.6.0"
  },
  "devDependencies": {
    "@types/express": "~4.17.1",
    "@types/cors": "~2.8.6",
    "@types/morgan": "~1.7.37",
    "@types/node-fetch": "~2.5.2",
    "@types/cheerio": "~0.22.13",
    "ts-node-dev": "~1.0.0-pre.43",
    "typescript": "~3.6.4"
  }
}
