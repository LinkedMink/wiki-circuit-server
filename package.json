{
  "name": "wiki-circuit-server",
  "version": "1.1.0",
  "description": "The server component of the Wiki Circuit project",
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan@linkedmink.space"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/wiki-circuit-server"
  },
  "private": true,
  "scripts": {
    "start": "npm run build && npm run startApp",
    "build": "tsc",
    "startApp": "node -r dotenv/config ./build/src/App.js",
    "watch": "ts-node-dev --respawn --transpileOnly ./src/App.ts",
    "test": "cross-env NODE_ENV=unitTest jest",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src ./tests",
    "lintFix": "eslint -c .eslintrc.js --ext .ts --fix ./src ./tests",
    "containerize": "npm run build && docker build -t linkedmink/wiki-circuit-server ."
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cheerio": "~1.0.0-rc.3",
    "cors": "~2.8.5",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "express-ws": "~4.0.0",
    "ioredis": "~4.16.1",
    "morgan": "~1.10.0",
    "multilevel-aging-cache": "github:LinkedMink/multilevel-aging-cache#master",
    "node-fetch": "~2.6.0",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "~0.22.17",
    "@types/cors": "~2.8.6",
    "@types/express": "~4.17.6",
    "@types/express-ws": "~3.0.0",
    "@types/ioredis": "~4.14.9",
    "@types/jest": "~25.2.1",
    "@types/morgan": "~1.9.0",
    "@types/node-fetch": "~2.5.5",
    "@types/ws": "~7.2.3",
    "@typescript-eslint/eslint-plugin": "~2.27.0",
    "@typescript-eslint/parser": "~2.27.0",
    "cross-env": "~7.0.2",
    "eslint": "~6.8.0",
    "jest": "~25.3.0",
    "ts-jest": "~25.3.1",
    "ts-node-dev": "~1.0.0-pre.44",
    "typescript": "~3.8.3"
  },
  "optionalDependencies": {
    "bufferutil": "~4.0.1",
    "utf-8-validate": "~5.0.2"
  }
}
